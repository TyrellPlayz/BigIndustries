{
  "multipart": [
    {
      "apply": {
        "model": "big_industries:block/fluid_pipe/none"
      }
    },
    {
      "when": {
        "north": "false", "east": "false", "south": "false", "west": "false",
        "down": "false", "up": "false"
      },
      "apply": {
        "model": "big_industries:block/fluid_pipe/fluid_pipe"
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "big_industries:block/fluid_pipe/side"
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "big_industries:block/fluid_pipe/side",
        "y": 90
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "big_industries:block/fluid_pipe/side",
        "y": 180
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "big_industries:block/fluid_pipe/side",
        "y": -90
      }
    },
    {
      "when": {
        "up": "true"
      },
      "apply": {
        "model": "big_industries:block/fluid_pipe/side",
        "x": -90
      }
    },
    {
      "when": {
        "down": "true"
      },
      "apply": {
        "model": "big_industries:block/fluid_pipe/side",
        "x": 90
      }
    }
  ]
}